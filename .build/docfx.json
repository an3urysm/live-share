{
  "build": {
    "content": [
      {
        "src": "..",
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "exclude": [
          "build/**"
        ]
      }
    ],
    "resource": [
      {
        "src": "..",
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "build/**"
        ]
      }
    ],
    "globalMetadata": {
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/live-share",
      "feedback_product_url": "https://aka.ms/vsls-issues/",
      "titleSuffix": "Visual Studio Live Share"
    },
    "dest": "vslivesharedocs"  
  }
}
